{
  "name": "@d11k-ts/crypto",
  "version": "0.1.0",
  "description": "crypto module for d11k chain",
  "keywords": [
    "d11k",
    "crypto",
    "dojima"
  ],
  "author": "bhaagiKenpachi <bhagath.reddy@dojima.network>",
  "homepage": "https://github.com/dojimanetwork/d11k-ts#readme",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "jsnext:main": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "!lib/__tests__"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dojimanetwork/d11k-ts.git"
  },
  "scripts": {
    "clean": "rimraf lib/**",
    "build": "yarn clean && rollup -c",
    "lint": "eslint \"{src,__tests__}/**/*.ts\" --fix --max-warnings 0",
    "test": "jest --coverage",
    "prepublishOnly": "yarn build"
  },
  "bugs": {
    "url": "https://github.com/dojimanetwork/d11k-ts/issues"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/lib",
      "<rootDir>/node_modules/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/bech32": "^1.1.2",
    "@types/bip39": "^3.0.0",
    "@types/crypto-js": "^4.0.2",
    "@types/hdkey": "^2.0.0",
    "@types/node": "^14.14.31",
    "@types/uuid": "^8.3.1"
  },
  "dependencies": {
    "bech32": "^2.0.0",
    "bip39": "^3.0.4",
    "crypto-js": "^4.1.1",
    "foundry-primitives": "^0.2.1",
    "hdkey": "^2.0.1",
    "uuid": "^8.3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "da5485a97c6bbb2ca588d4b470fbdea769e54ce3"
}
